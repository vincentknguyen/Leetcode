    "nums1 = [-2,1,-3,4,-1,2,1,-5,4]\n",
    "\n",
    "#Kadane's algorithm\n",
    "def maxSubarr ay(nums):\n",
    "    max_sum = float('-inf')\n",
    "    cur_sum = 0\n",
    "    for n in nums:\n",
    "        cur_sum = max(cur_sum + n, n)\n",
    "        max_sum = max(max_sum, cur_sum)\n",
    "    return max_sum\n",
    "\n",
    "print(maxSubarray(nums1))\n",
    "\n",
